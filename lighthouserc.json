{
	"ci": {
		"collect": {
			"startServerCommand": "npm run dev",
			"url": ["http://localhost:5173"]
		},
		"assert": {
			"assertions": {
				"categories:performance": [
					"warn",
					{
						"minScore": 0.9
					}
				],
				"categories:accessibility": [
					"error",
					{
						"minScore": 0.9
					}
				]
			}
		}
	}
}
